<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.web.WebView?>
<AnchorPane xmlns="http://javafx.com/javafx/11.0.14-internal"
            fx:id="chartsAnchorPane"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.fxdemo.controller.ChartsController">
    <children>
        <VBox fx:id="chartsVbox" style="-fx-background-color: #aaaa43;" AnchorPane.bottomAnchor="0.0"
              AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <Label fx:id="chartsLabel" alignment="CENTER" text="采集中，采集时长：4小时34分钟23秒，采集次数：2333420">
                    <font>
                        <Font size="21.0"/>
                    </font>
                </Label>
                <WebView fx:id="webView" VBox.vgrow="ALWAYS"/>
            </children>
        </VBox>
    </children>
</AnchorPane>
